<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<Ploy>
    <Nodes>
        <StartNode id="start" />

        <ActivityNode id="activity1" job="testJob" name="Activity 1">
            <Inputs>
                <Input name="message" from="data1" output="value" />
            </Inputs>
        </ActivityNode>

        <ActivityNode id="activity2" job="testJob" name="Activity 2">
            <Inputs>
                <Input name="message" from="activity1" output="message" />
            </Inputs>
        </ActivityNode>

        <ActivityNode id="activity3" job="testJob" name="Activity 3">
            <Inputs>
                <Input name="message" from="activity2" output="message" />
            </Inputs>
        </ActivityNode>

        <EndNode id="end" />

        <DataNode id="data1" type="string" value="Artjoms" />

        <ScriptNode id="script1" />
